';\nimport Loader from '../../common/loader';\nimport { normalizeUrl, toCurrency } from '../../../_helpers/formatting';\nimport Breadcrumb from '../../common/breadcrumb';\nimport SearchItem from \"./SearchItem\";\n\nconst SearchList = (props:searchProps) => {\n  const { querySearch } = useRequestData();\n  const { response, loading, error, loadData } = useFetch();\n  const [ categories, setCategories ] = useState<string[]>( [] );\n  const [ products, setProducts ] = useState<itemsProps[]>( [] );\n\n  \n  useEffect(() => {\n    if( querySearch && querySearch !== '' ){\n      loadData({\n        method: 'GET',\n        path: `/api/items?q=${normalizeUrl(querySearch)}`,\n      });\n    }\n  }, [querySearch]);\n\n  useEffect(() => {\n    if( response ){\n      setProducts( response.data.items );\n      setCategories( response.data.categories );\n    }\n  }, [response]);\n\n\n  return (\n    <React.Fragment>\n      { querySearch !== '' ?\n          <React.Fragment>\n            <Helmet>\n              <title>Resultados obtenidos en la búsqueda: {querySearch} </title>\n              <meta name=\"description\" content={'Resultados obtenidos en la búsqueda: ' + querySearch} />\n            </Helmet>\n            { error ?\n                'Hubo un error al obtener la información'\n              :\n                (loading ?\n                  <Loader />\n                :\n                  <div className=\"container\">\n                    { categories &&\n                        <Breadcrumb categories={categories} />\n                    }\n                    <div className=\"resultBox\">\n                      { products && products.length !== 0 ?\n                          products.map((item:itemsProps, index:number) => {\n                            return (\n                              <SearchItem key={index} {...item} />\n                            )\n                          })\n                        :\n                          <div className=\"noResults\">Lo sentimos, no hay productos que coincidan con tu búsqueda</div>\n                      }\n                    </div>\n                  </div>\n                ) \n            }\n          </React.Fragment>\n        :\n          ''\n      }\n    </React.Fragment>\n  )\n}\n\nexport default SearchList;","/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/app/item/Item.tsx",["125","126","127","128","129","130"],"import React, { useEffect, useState } from 'react';\nimport { Helmet } from 'react-helmet';\nimport { Link, useParams } from 'react-router-dom';\nimport { itemDetailProps } from '../../../type';\nimport { getDecimals, normalizeUrl, toCurrency } from '../../../_helpers/formatting';\nimport useFetch from '../../../_services/fetch';\nimport Breadcrumb from '../../common/breadcrumb';\nimport Button from '../../common/forms/button';\nimport Loader from '../../common/loader';\nimport ItemDetail from './ItemDetail';\n\ninterface parameterProps {\n  id: string,\n}\n\nconst Item = () => {\n  const { response, loading, error, loadData, loadDataExtra, setLoading} = useFetch();\n  const [ product, setProduct ] = useState<itemDetailProps>(  );\n  const [ categories, setCategories ] = useState<string[]>( [] );\n  let params:parameterProps = useParams();\n\n  \n  useEffect(() => {\n    if( params && params.id ){\n      loadData({\n        method: 'GET',\n        path: `/api/items/${params.id}`,\n      });\n    }\n  }, [params]);\n\n  useEffect(() => {\n    if( response ){\n      const prodTitle:string = response.data.item.title;\n      setProduct( response.data.item );\n\n      loadDataExtra({\n        method: 'GET',\n        path: `/api/items?q=${normalizeUrl(response.data.item.title)}`,\n      }).then(response => {\n        if( response?.data.categories[0] ){\n          setCategories( [response?.data.categories[0], prodTitle] );\n        }\n        setLoading(false);\n      })\n    }\n  }, [response]);\n  \n  return (\n    <React.Fragment>\n      \n      { product &&\n          <Helmet>\n            <title>{product.title} - Mercado Libre Challenge</title>\n            <meta name=\"description\" content={ product.description+' - Mercado Libre Challenge' } />\n          </Helmet>\n      }\n      \n      { error ?\n          'Hubo un error al obtener la información'\n        :\n          (loading ?\n            <Loader />\n          :\n            <div className=\"container\">\n              <Breadcrumb categories={categories} />\n              <div className=\"resultBox\">\n                {  product ?\n                    <ItemDetail {...product} />\n                  :\n                    <div className=\"noResults\">Lo sentimos, no hay productos que coincidan con tu búsqueda</div>\n                }\n              </div>\n            </div>\n          ) \n      }\n    </React.Fragment>\n  )\n}\n\nexport default Item;","/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/_router/router.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/common/header/Header.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/common/forms/input/Input.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/common/forms/button/Button.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/common/searchBar/SearchBar.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/app/item/index.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/_services/request.ts",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/_services/url.ts",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/app/search/index.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-front/src/components/common/header/index.tsx",[],"/Users/gutdays/Documents/Dev/MercadoLibre/ml-challenge/ml-fron